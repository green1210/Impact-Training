class Animal {
    void eat() { 
        System.out.println("Animal is Eating.");
    }
}

class Dog extends Animal {
    @Override
    void eat() {  
        System.out.println("Dog is Eating.");
    }
}

public class Main {
    public static void main(String[] args) {
        Animal animal = new Animal();   
        Animal dog = new Dog();         
        
        animal.eat();  
        dog.eat();     
    }
}
