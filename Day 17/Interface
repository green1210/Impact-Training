interface A{
    int method2(int a, int b);
        }
        
    class B implements A{
        
        public int method2(int a, int b){
            return a+b;
        }
    }
    
    class Main{
        public static void main(String[] args){
            B obj = new B();
            
            int res = obj.method2(1,2);
            System.out.println(res);
        }
    }
