//next greater elements

class Main {
    public static void main(String[] args) {
        int[] arr = {2, 8, 1, 4, 3, 6};
        
        for(int i = 0;i<arr.length;i++){
            int nge = -1;
            for(int j= i+1;j<arr.length;j++){
                if(arr[i]<arr[j]){
                    nge = arr[j];
                    break;
                }
                }
                  System.out.println(" NGE of " + arr[i] + " is " + nge);
            }
          
        }
        
}
